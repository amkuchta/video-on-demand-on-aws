{
  "app": "npx ts-node --prefer-ts-exts bin/aws-vod-cdk.ts",
  "context": {
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true",
    "@aws-cdk/aws-ecr-assets:dockerIgnoreSupport": true,
    "@aws-cdk/aws-secretsmanager:parseOwnedSecretName": true,
    "@aws-cdk/aws-s3:grantWriteWithoutAcl": true,
    "@aws-cdk/aws-ecs-patterns:removeDefaultDesiredCount": true,
    "@aws-cdk/aws-rds:lowercaseDbIdentifier": true,
    "adminEmail": "amkuchta@gmail.com",
    "workflowTrigger": "MetadataFile",
    "glacier": "DISABLED",
    "frameCapture": true,
    "enableMediaPackage": true,
    "enableSns": true,
    "enableSqs": true,
    "acceleratedTranscoding": "PREFERRED",
    "stackName": "TrineYogaVod",
    "stackStage": "Dev",
    "stackVersion": "0.1.1",
    "cloudFrontDomainBase": "trineyoga.com",
    "videosSubDomain": "videos",
    "authenticationSubDomain": "auth",
    "prependDomainWithStackStage": true,
    "hostedZoneId": "Z0673592U4C957ZHR01X",
    "sendAnonymousMetrics": true,
    "deployAppSync": true,
    "deployCognito": true
  }
}
